@use 'src/scss/reset';\n\n//Fonts\n@import url('https://fonts.googleapis.com/css2?family=Barlow:wght@300;400;600;900&display=swap');\n\n\n//Colors\n$main_bg_color : #000000;\n$accent_color : #f9f871;\n$primary_color : #7a00b4;\n$primary_color_hover : #660093;\n$main_font_color: #fefefe;\n\n//Font properties\n$font_light : 300;\n$font_regular: 400;\n$font_semi: 600;\n$font_bold : 900;\n\n:root{\n  font-family: 'Barlow', sans-serif;\n}\n\nbody {\n  background: $main_bg_color;\n  color: $main_font_color;\n  font-weight: $font_regular;\n  font-size: 1.1rem;\n  line-height: 1.5;\n}\n\nh1,\nh2,\nh3{\n  text-transform: uppercase;\n  font-weight: $font_semi;\n  line-height: 0.9;\n}\n\n\nul{\n  list-style: none;\n}\n\na{\n  text-decoration: none;\n  color: inherit;\n}\n\n\n\n.wrapper {\n  width: 100%;\n  margin: 0 auto;\n}\n\n.light{\n  font-weight: $font_light;\n}\n.regular{\n  font-weight: $font_regular;\n}\n.semi{\n  font-weight: $font_semi;\n}\n.bold{\n  font-weight: $font_bold;\n}\n\n\n.color__accent{\n  color: $accent_color;\n}\n\n\nheader{\n  display: flex;\n  justify-content: space-between;\n  padding: 0.2rem 2.5rem 0.2rem 1.5rem;\n  //background-color: rgb(0 0 0 / 0.5);\n  background-image: linear-gradient(to bottom, rgba(0,0,0,1),rgba(0,0,0,0.1));\n}\n\n.header__locked{\n  position: fixed;\n  top: 0;\n  width: 100%\n}\n\n\n.hero__image{\n  background-image: url(\"../img/hero-montain-bw-gradient_fitted.png\");\n  background-size: 2000px;\n  background-repeat: no-repeat;\n  background-position: 50% 0%;\n  min-height: 550px;\n  height: 100%;\n  width: 100%;\n}\n\n\n.logo {\n  display: flex;\n}\n\n.svg__logo{\n  margin: auto;\n  max-height: 5vh;\n  opacity: 0.6;\n}\n\n.menu{\n  display: flex;\n}\n\n.menu__option{\n  line-height: 0.9;\n  padding: 5px;\n  font-size: 1em;\n}\n\n.menu__selected{\n  border-bottom: 0.1em solid $accent_color;\n}\n\n\n.btn {\n  text-align: center;\n  text-transform: uppercase;\n  padding: 0.75rem 1rem;\n  font-weight: $font_semi;\n  font-size: 1.125rem;\n  margin: 2rem;\n  background-color: $primary_color;\n  border-radius: 0.75rem;\n  transition: all 150ms ease;\n}\n\n.btn:hover{\n  background-color: $primary_color_hover;\n  box-shadow: 0 0 0.25rem 0.1rem rgba(255, 255, 255, 0.15);\n  transition: all 150ms ease;\n}\n\n\n\n@media screen and (min-width: 2000px) {\n\n  .wrapper {\n    width: 80%;\n    margin: 0 auto;\n  }\n}","scss/reset.scss":"/* Box sizing rules */\n*,\n*::before,\n*::after {\n  box-sizing: border-box;\n}\n\n/* Remove default margin */\nbody,\nh1,\nh2,\nh3,\nh4,\np,\nfigure,\nblockquote,\ndl,\ndd {\n  margin: 0;\n}\n\n/* Remove list styles on ul, ol elements with a list role, which suggests default styling will be removed */\nul[role='list'],\nol[role='list'] {\n  list-style: none;\n}\n\n/* Set core root defaults */\nhtml:focus-within {\n  scroll-behavior: smooth;\n}\n\n/* Set core body defaults */\nbody {\n  min-height: 100vh;\n  text-rendering: optimizeSpeed;\n  line-height: 1.5;\n}\n\n/* A elements that don't have a class get default styles */\na:not([class]) {\n  text-decoration-skip-ink: auto;\n}\n\n/* Make images easier to work with */\nimg,\npicture {\n  max-width: 100%;\n  display: block;\n}\n\n/* Inherit fonts for inputs and buttons */\ninput,\nbutton,\ntextarea,\nselect {\n  font: inherit;\n}\n\n/* Remove all animations, transitions and smooth scroll for people that prefer not to see them */\n@media (prefers-reduced-motion: reduce) {\n  html:focus-within {\n    scroll-behavior: auto;\n  }\n\n  *,\n  *::before,\n  *::after {\n    animation-duration: 0.01ms !important;\n    animation-iteration-count: 1 !important;\n    transition-duration: 0.01ms !important;\n    scroll-behavior: auto !important;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"908eca213f578469c4665099f2e52919","cacheData":{"env":{}}}